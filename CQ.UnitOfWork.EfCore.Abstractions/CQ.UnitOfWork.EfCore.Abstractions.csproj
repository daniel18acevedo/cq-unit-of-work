<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
		<PackageId>CQ.UnitOfWork.EfCore.Abstractions</PackageId>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/daniel18acevedo/cq-unit-of-work</RepositoryUrl>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>Unit of Work with EfCore Abstractions</Title>
		<Authors>Code Quality</Authors>
		<Company>Code Quality</Company>
